# This file is managed by Puppet - DO NOT EDIT

auto lo
iface lo inet loopback

iface <%= $::bobcat::network::interfaces::device %> inet static
  address <%= $::bobcat::network::interfaces::address %>
  netmask <%= $::bobcat::network::interfaces::netmask %>
  gateway <%= $::bobcat::network::interfaces::gateway %>
<% $::bobcat::network::interfaces::nameservers.each |$server| { -%>
  dns-nameserver <%= $server %>
<% } -%>
